{
  "id": null,
  "uid": "example-dashboard",
  "title": "Sexy Grafana Dashboard",
  "tags": ["example"],
  "timezone": "browser",
  "schemaVersion": 27,
  "version": 0,
  "refresh": "5s",
  "panels": [
    {
      "type": "graph",
      "title": "CPU Usage",
      "gridPos": {
        "x": 0,
        "y": 0,
        "w": 12,
        "h": 8
      },
      "id": 1,
      "datasource": "Prometheus",
      "targets": [
        {
          "expr": "100 - (avg by (instance) (irate(node_cpu_seconds_total{mode=\"idle\"}[5m])) * 100)",
          "interval": "",
          "legendFormat": "{{instance}}",
          "refId": "A"
        }
      ],
      "lines": true,
      "linewidth": 2,
      "fill": 1,
      "tooltip": {
        "shared": true,
        "sort": 0,
        "value_type": "individual"
      },
      "xaxis": {
        "show": true,
        "mode": "time",
        "name": null,
        "values": []
      },
      "yaxis": {
        "show": true,
        "format": "percent",
        "label": "CPU Usage",
        "logBase": 1,
        "min": 0,
        "max": 100
      }
    },
    {
      "type": "singlestat",
      "title": "Memory Usage",
      "gridPos": {
        "x": 12,
        "y": 0,
        "w": 12,
        "h": 8
      },
      "id": 2,
      "datasource": "Prometheus",
      "targets": [
        {
          "expr": "100 - (node_memory_MemAvailable_bytes / node_memory_MemTotal_bytes * 100)",
          "interval": "",
          "refId": "A"
        }
      ],
      "format": "percent",
      "prefix": "",
      "postfix": "%",
      "valueName": "current",
      "colorBackground": true,
      "colorValue": false,
      "colors": [
        "#299c46",
        "#e5ac0e",
        "#d44a3a"
      ],
      "thresholds": "70,90",
      "sparkline": {
        "show": true,
        "full": false,
        "lineColor": "rgb(31, 120, 193)",
        "fillColor": "rgba(31, 118, 189, 0.18)"
      }
    },
    {
      "type": "graph",
      "title": "Disk Usage",
      "gridPos": {
        "x": 0,
        "y": 8,
        "w": 12,
        "h": 8
      },
      "id": 3,
      "datasource": "Prometheus",
      "targets": [
        {
          "expr": "100 - (node_filesystem_avail_bytes{fstype!=\"tmpfs\",fstype!=\"overlay\"} / node_filesystem_size_bytes{fstype!=\"tmpfs\",fstype!=\"overlay\"} * 100)",
          "interval": "",
          "legendFormat": "{{instance}}",
          "refId": "A"
        }
      ],
      "lines": true,
      "linewidth": 2,
      "fill": 1,
      "tooltip": {
        "shared": true,
        "sort": 0,
        "value_type": "individual"
      },
      "xaxis": {
        "show": true,
        "mode": "time",
        "name": null,
        "values": []
      },
      "yaxis": {
        "show": true,
        "format": "percent",
        "label": "Disk Usage",
        "logBase": 1,
        "min": 0,
        "max": 100
      }
    },
    {
      "type": "graph",
      "title": "Network Traffic",
      "gridPos": {
        "x": 12,
        "y": 8,
        "w": 12,
        "h": 8
      },
      "id": 4,
      "datasource": "Prometheus",
      "targets": [
        {
          "expr": "rate(node_network_receive_bytes_total{device!~\"lo\"}[5m])",
          "interval": "",
          "legendFormat": "Receive - {{instance}}",
          "refId": "A"
        },
        {
          "expr": "rate(node_network_transmit_bytes_total{device!~\"lo\"}[5m])",
          "interval": "",
          "legendFormat": "Transmit - {{instance}}",
          "refId": "B"
        }
      ],
      "lines": true,
      "linewidth": 2,
      "fill": 1,
      "tooltip": {
        "shared": true,
        "sort": 0,
        "value_type": "individual"
      },
      "xaxis": {
        "show": true,
        "mode": "time",
        "name": null,
        "values": []
      },
      "yaxis": {
        "show": true,
        "format": "bytes",
        "label": "Network Traffic",
        "logBase": 1,
        "min": null,
        "max": null
      }
    },
    {
      "type": "stat",
      "title": "HTTP Status Code",
      "gridPos": {
        "x": 0,
        "y": 16,
        "w": 6,
        "h": 4
      },
      "id": 5,
      "datasource": "Prometheus",
      "targets": [
        {
          "expr": "probe_http_status_code{instance=~\"$target\"}",
          "interval": "$interval",
          "intervalFactor": 1,
          "refId": "A"
        }
      ],
      "options": {
        "colorMode": "none",
        "graphMode": "none",
        "justifyMode": "auto",
        "orientation": "horizontal",
        "reduceOptions": {
          "calcs": ["lastNotNull"],
          "fields": "",
          "values": false
        },
        "textMode": "auto"
      },
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "thresholds"
          },
          "mappings": [
            {
              "options": {
                "match": "null",
                "result": {
                  "text": "N/A"
                },
                "type": "special"
              }
            }
          ],
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "color": "green",
                "value": null
              },
              {
                "color": "red",
                "value": 80
              }
            ]
          },
          "unit": "none"
        },
        "overrides": []
      }
    },
    {
      "type": "stat",
      "title": "Average Probe Duration",
      "gridPos": {
        "x": 6,
        "y": 16,
        "w": 6,
        "h": 4
      },
      "id": 6,
      "datasource": "Prometheus",
      "targets": [
        {
          "expr": "avg(probe_duration_seconds{instance=~\"$target\"})",
          "interval": "$interval",
          "intervalFactor": 1,
          "refId": "A"
        }
      ],
      "options": {
        "colorMode": "none",
        "graphMode": "none",
        "justifyMode": "auto",
        "orientation": "horizontal",
        "reduceOptions": {
          "calcs": ["lastNotNull"],
          "fields": "",
          "values": false
        },
        "textMode": "auto"
      },
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "thresholds"
          },
          "mappings": [
            {
              "options": {
                "match": "null",
                "result": {
                  "text": "N/A"
                },
                "type": "special"
              }
            }
          ],
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "color": "green",
                "value": null
              },
              {
                "color": "red",
                "value": 80
              }
            ]
          },
          "unit": "s"
        },
        "overrides": []
      }
    },
    {
      "type": "stat",
      "title": "Average DNS Lookup",
      "gridPos": {
        "x": 12,
        "y": 16,
        "w": 6,
        "h": 4
      },
      "id": 7,
      "datasource": "Prometheus",
      "targets": [
        {
          "expr": "avg(probe_dns_lookup_time_seconds{instance=~\"$target\"})",
          "interval": "$interval",
          "intervalFactor": 1,
          "refId": "A"
        }
      ],
      "options": {
        "colorMode": "none",
        "graphMode": "none",
        "justifyMode": "auto",
        "orientation": "horizontal",
        "reduceOptions": {
          "calcs": ["lastNotNull"],
          "fields": "",
          "values": false
        },
        "textMode": "auto"
      },
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "thresholds"
          },
          "mappings": [
            {
              "options": {
